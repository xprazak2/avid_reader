<% provide(:title, 'New post')%>
<h1>Create new post</h1>
<div class="row"> 
 <div class="span6 offset3">
  <%= form_for([@book, @post]) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :content %>
    <%= f.text_area :content %>

    <%= f.submit "Create new post", class: "btn btn-large btn-inverse"%>
  <% end %>
 </div>
</div>
